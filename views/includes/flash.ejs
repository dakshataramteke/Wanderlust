<% if(successMsg && successMsg.length >0){ %>
<div class="alert alert-success alert-dismissible fade show col-6 offset-3" role="alert">
  <p class="mb-0 mx-auto"><%= successMsg %></p>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %>

<% if(errorMsg && errorMsg.length >0){ %>
<div class="alert alert-danger col-6 offset-3" role="alert">
 <p class="mb-0 mx-auto"><%= errorMsg %></p>
  </div>
  <% } %>